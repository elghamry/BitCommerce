<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:elevation="16dp"
            tools:context=".fragments.ConfirmationFragment">


        <androidx.core.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="0dp"


                android:layout_marginBottom="24dp"
                app:layout_constraintBottom_toTopOf="@+id/confirm_btn"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">
            <!--        android:background="@drawable/white_bg"-->

            <androidx.constraintlayout.widget.ConstraintLayout

                    android:layout_width="match_parent"
                    android:layout_height="match_parent"


                    >

                <!-- TODO: Update blank fragment layout -->
                <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/hello_blank_fragment"
                        android:visibility="gone" />


                <TextView
                        android:id="@+id/tvThankYou"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="24dp"

                        android:layout_marginTop="30dp"
                        android:drawablePadding="10dp"
                        android:elevation="5dp"
                        android:fontFamily="@font/grenale_conbla"
                        android:text="@string/thank_you"
                        android:textColor="@color/white"
                        android:textSize="32sp"
                        android:textStyle="bold"
                        android:transitionName="tvThankYou"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                <TextView
                        android:id="@+id/tvOrderPlaced"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="30dp"
                        android:drawablePadding="10dp"
                        android:elevation="5dp"
                        android:fontFamily="@font/grenale_conreg"

                        android:text="@string/your_order_has_been_placed"
                        android:textColor="@color/white"
                        android:textSize="30sp"
                        android:textStyle="normal"
                        android:transitionName="tvThankYou"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tvThankYou" />


                <androidx.cardview.widget.CardView
                        android:id="@+id/cv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="24dp"


                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="24dp"

                        app:cardBackgroundColor="@color/white"

                        app:cardCornerRadius="10dp"
                        app:cardElevation="2dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tvOrderPlaced">

                    <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:layout_marginStart="24dp"
                            android:layout_marginTop="16dp"
                            android:layout_marginEnd="24dp"
                            android:layout_marginBottom="16dp">

                        <androidx.constraintlayout.widget.ConstraintLayout
                                android:id="@+id/order_no_container"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="4dp"
                                app:layout_constraintBottom_toTopOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent">


                            <TextView
                                    android:id="@+id/order_no"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="16dp"

                                    android:layout_marginTop="4dp"
                                    android:fontFamily="@font/grenale_conbla"
                                    android:text="@string/order_no"

                                    android:textColor="@color/text_header"

                                    android:textSize="16sp"
                                    android:textStyle="bold"

                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                            <TextView
                                    android:id="@+id/order_no_value"
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:layout_marginStart="4dp"
                                    android:layout_marginTop="8dp"
                                    android:fontFamily="@font/ubuntu_regular"
                                    android:text="11111111"
                                    android:textColor="@color/text_header"
                                    android:textSize="12sp"
                                    android:visibility="visible"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toEndOf="@id/order_no"
                                    app:layout_constraintTop_toTopOf="parent" />

                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <androidx.constraintlayout.widget.ConstraintLayout
                                android:id="@+id/order_date_container"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="4dp"


                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@id/order_no_container">


                            <TextView
                                    android:id="@+id/order_date"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="16dp"

                                    android:layout_marginTop="4dp"
                                    android:fontFamily="@font/grenale_conbla"
                                    android:text="@string/order_date"

                                    android:textColor="@color/text_header"

                                    android:textSize="16sp"
                                    android:textStyle="bold"

                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                            <TextView
                                    android:id="@+id/order_date_value"
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:layout_marginStart="4dp"
                                    android:layout_marginTop="8dp"
                                    android:fontFamily="@font/ubuntu_regular"
                                    android:text="25 JUN 19"
                                    android:textColor="@color/text_header"
                                    android:textSize="12sp"
                                    android:visibility="visible"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toEndOf="@id/order_date"
                                    app:layout_constraintTop_toTopOf="parent" />

                        </androidx.constraintlayout.widget.ConstraintLayout>


                        <androidx.constraintlayout.widget.ConstraintLayout
                                android:id="@+id/shipping_date_container"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="4dp"


                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintTop_toBottomOf="@id/order_no_container">


                            <TextView
                                    android:id="@+id/shipping_date"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="16dp"

                                    android:layout_marginTop="4dp"
                                    android:fontFamily="@font/grenale_conbla"
                                    android:text="@string/shipping_date"

                                    android:textColor="@color/text_header"

                                    android:textSize="16sp"
                                    android:textStyle="bold"


                                    app:layout_constraintEnd_toStartOf="@id/shipping_date_value"
                                    app:layout_constraintTop_toTopOf="parent" />

                            <TextView
                                    android:id="@+id/shipping_date_value"
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:layout_marginStart="4dp"
                                    android:layout_marginTop="8dp"
                                    android:layout_marginEnd="12dp"
                                    android:fontFamily="@font/ubuntu_regular"
                                    android:text="30 JUN 19"
                                    android:textColor="@color/text_header"
                                    android:textSize="12sp"
                                    android:visibility="visible"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                        </androidx.constraintlayout.widget.ConstraintLayout>


                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.cardview.widget.CardView>

                <TextView
                        android:id="@+id/tvYourOrder"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="56dp"

                        android:layout_marginTop="16dp"

                        android:drawablePadding="10dp"
                        android:elevation="5dp"
                        android:fontFamily="@font/grenale_conbla"
                        android:text="@string/your_order"
                        android:textColor="@color/white"
                        android:textSize="20sp"
                        android:textStyle="bold"

                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/cv" />


                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/your_order_container"
                        android:layout_width="0dp"

                        android:layout_height="wrap_content"

                        android:layout_marginStart="56dp"
                        android:orientation="vertical"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tvYourOrder">


                    <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"

                            android:layout_marginTop="6dp"
                            android:orientation="vertical"
                            app:layout_constraintStart_toStartOf="parent"

                            app:layout_constraintTop_toTopOf="parent">

                        <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:layout_marginTop="6dp"
                                android:orientation="horizontal"
                                app:layout_constraintStart_toStartOf="parent"

                                app:layout_constraintTop_toTopOf="parent">

                            <TextView
                                    android:id="@+id/product_no"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="4dp"
                                    android:fontFamily="@font/ubuntu_regular"
                                    android:lines="1"
                                    android:maxLength="25"
                                    android:maxLines="1"
                                    android:minLines="1"
                                    android:text="X1"
                                    android:textColor="@color/white"

                                    android:textSize="15sp" />

                            <TextView
                                    android:id="@+id/product_title"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="12dp"
                                    android:fontFamily="@font/grenale_conbla"

                                    android:maxLength="15"
                                    android:maxLines="1"
                                    android:text="Rocky Road"
                                    android:textColor="@color/white"

                                    android:textSize="15sp"
                                    android:textStyle="bold" />
                        </LinearLayout>


                        <TextView
                                android:id="@+id/product_description"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="3dp"
                                android:fontFamily="@font/ubuntu_regular"
                                android:lines="1"
                                android:maxLength="25"
                                android:maxLines="1"
                                android:minLines="1"
                                android:text="High protein bars (Peanut Butter) "
                                android:textColor="@color/white"

                                android:textSize="12sp" />


                    </LinearLayout>

                    <TextView
                            android:id="@+id/price"
                            android:layout_width="0dp"
                            android:layout_height="19dp"

                            android:layout_marginEnd="56dp"
                            android:fontFamily="@font/ubuntu_medium"
                            android:text="@string/price_demo"
                            android:textColor="@color/white"

                            android:textSize="14sp"
                            android:textStyle="bold"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />


                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/your_order2_container"
                        android:layout_width="0dp"

                        android:layout_height="wrap_content"

                        android:layout_marginStart="56dp"
                        android:orientation="vertical"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/your_order_container">


                    <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"

                            android:layout_marginTop="6dp"
                            android:orientation="vertical"
                            app:layout_constraintStart_toStartOf="parent"

                            app:layout_constraintTop_toTopOf="parent">

                        <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:layout_marginTop="6dp"
                                android:orientation="horizontal"
                                app:layout_constraintStart_toStartOf="parent"

                                app:layout_constraintTop_toTopOf="parent">

                            <TextView
                                    android:id="@+id/product_no2"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="4dp"
                                    android:fontFamily="@font/ubuntu_regular"
                                    android:lines="1"
                                    android:maxLength="25"
                                    android:maxLines="1"
                                    android:minLines="1"
                                    android:text="X1"
                                    android:textColor="@color/white"

                                    android:textSize="15sp" />

                            <TextView
                                    android:id="@+id/product_title2"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="12dp"
                                    android:fontFamily="@font/grenale_conbla"

                                    android:maxLength="15"
                                    android:maxLines="1"
                                    android:text="Rocky Road"
                                    android:textColor="@color/white"

                                    android:textSize="15sp"
                                    android:textStyle="bold" />
                        </LinearLayout>


                        <TextView
                                android:id="@+id/product_description2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="3dp"
                                android:fontFamily="@font/ubuntu_regular"
                                android:lines="1"
                                android:maxLength="25"
                                android:maxLines="1"
                                android:minLines="1"
                                android:text="High protein bars (Peanut Butter) "
                                android:textColor="@color/white"

                                android:textSize="12sp" />


                    </LinearLayout>

                    <TextView
                            android:id="@+id/pric2"
                            android:layout_width="0dp"
                            android:layout_height="19dp"

                            android:layout_marginEnd="56dp"
                            android:fontFamily="@font/ubuntu_medium"
                            android:text="@string/price_demo"
                            android:textColor="@color/white"

                            android:textSize="14sp"
                            android:textStyle="bold"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />


                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/your_order_container3"
                        android:layout_width="0dp"

                        android:layout_height="wrap_content"

                        android:layout_marginStart="56dp"
                        android:orientation="vertical"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/your_order2_container">


                    <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"

                            android:layout_marginTop="6dp"
                            android:orientation="vertical"
                            app:layout_constraintStart_toStartOf="parent"

                            app:layout_constraintTop_toTopOf="parent">

                        <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:layout_marginTop="6dp"
                                android:orientation="horizontal"
                                app:layout_constraintStart_toStartOf="parent"

                                app:layout_constraintTop_toTopOf="parent">

                            <TextView
                                    android:id="@+id/product_no3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="4dp"
                                    android:fontFamily="@font/ubuntu_regular"
                                    android:lines="1"
                                    android:maxLength="25"
                                    android:maxLines="1"
                                    android:minLines="1"
                                    android:text="X1"
                                    android:textColor="@color/white"

                                    android:textSize="15sp" />

                            <TextView
                                    android:id="@+id/product_title3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="12dp"
                                    android:fontFamily="@font/grenale_conbla"

                                    android:maxLength="15"
                                    android:maxLines="1"
                                    android:text="Rocky Road"
                                    android:textColor="@color/white"

                                    android:textSize="15sp"
                                    android:textStyle="bold" />
                        </LinearLayout>


                        <TextView
                                android:id="@+id/product_description3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="3dp"
                                android:fontFamily="@font/ubuntu_regular"
                                android:lines="1"
                                android:maxLength="25"
                                android:maxLines="1"
                                android:minLines="1"
                                android:text="High protein bars (Peanut Butter) "
                                android:textColor="@color/white"

                                android:textSize="12sp" />


                    </LinearLayout>

                    <TextView
                            android:id="@+id/price3"
                            android:layout_width="0dp"
                            android:layout_height="19dp"

                            android:layout_marginEnd="56dp"
                            android:fontFamily="@font/ubuntu_medium"
                            android:text="@string/price_demo"
                            android:textColor="@color/white"

                            android:textSize="14sp"
                            android:textStyle="bold"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />


                </androidx.constraintlayout.widget.ConstraintLayout>

                <View
                        android:id="@+id/line1"
                        android:layout_width="0dp"
                        android:layout_height=".5dp"
                        android:layout_marginStart="32dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="32dp"

                        android:background="@color/line_color"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/your_order_container3"

                        />

                <TextView
                        android:id="@+id/tvDeliveryAddress"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="56dp"

                        android:layout_marginTop="16dp"

                        android:drawablePadding="10dp"
                        android:elevation="5dp"
                        android:fontFamily="@font/grenale_conbla"
                        android:text="@string/delivery_address"
                        android:textColor="@color/white"
                        android:textSize="20sp"
                        android:textStyle="bold"

                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/line1" />

                <TextView
                        android:id="@+id/address_details"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="56dp"
                        android:layout_marginEnd="4dp"
                        android:fontFamily="@font/ubuntu_regular"


                        android:text="Firas Alateeq\nPrince Muhammad Ibn Abdulaziz Rd, Riyadh 13515\nSaudi Arabia\n966 561111111"
                        android:textColor="@color/white"
                        android:textSize="15sp"
                        app:layout_constraintStart_toStartOf="parent"

                        app:layout_constraintTop_toBottomOf="@id/tvDeliveryAddress" />

                <TextView
                        android:id="@+id/tvDeliveryMethod"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="56dp"

                        android:layout_marginTop="16dp"

                        android:drawablePadding="10dp"
                        android:elevation="5dp"
                        android:fontFamily="@font/grenale_conbla"
                        android:text="@string/delivery_address"
                        android:textColor="@color/white"
                        android:textSize="20sp"
                        android:textStyle="bold"

                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/address_details" />

                <TextView
                        android:id="@+id/Next_Day_Delivery"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="56dp"
                        android:layout_marginEnd="4dp"
                        android:fontFamily="@font/ubuntu_regular"


                        android:text="Next Day Delivery"
                        android:textColor="@color/white"
                        android:textSize="15sp"
                        app:layout_constraintStart_toStartOf="parent"

                        app:layout_constraintTop_toBottomOf="@id/tvDeliveryMethod" />


                <View
                        android:id="@+id/line2"
                        android:layout_width="0dp"
                        android:layout_height=".5dp"
                        android:layout_marginStart="32dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="32dp"

                        android:background="@color/line_color"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/Next_Day_Delivery"

                        />

                <TextView
                        android:id="@+id/tvPaymentDetails"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="56dp"

                        android:layout_marginTop="16dp"

                        android:drawablePadding="10dp"
                        android:elevation="5dp"
                        android:fontFamily="@font/grenale_conbla"
                        android:text="@string/delivery_address"
                        android:textColor="@color/white"
                        android:textSize="20sp"
                        android:textStyle="bold"

                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/line2" />

                <TextView
                        android:id="@+id/Cash_On_Delivery"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="56dp"
                        android:layout_marginEnd="4dp"
                        android:fontFamily="@font/ubuntu_regular"


                        android:text="Cash On Delivery"
                        android:textColor="@color/white"
                        android:textSize="15sp"
                        app:layout_constraintStart_toStartOf="parent"

                        app:layout_constraintTop_toBottomOf="@id/tvPaymentDetails" />

                <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/Cash_On_Delivery">

                    <TextView
                            android:id="@+id/tvPaymentTotal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="56dp"

                            android:layout_marginTop="16dp"

                            android:drawablePadding="10dp"
                            android:elevation="5dp"
                            android:fontFamily="@font/grenale_conbla"
                            android:text="Payment Total"
                            android:textColor="@color/white"
                            android:textSize="20sp"
                            android:textStyle="bold"

                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                            android:id="@+id/payment_total_value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="4dp"
                            android:layout_marginTop="16dp"
                            android:layout_marginEnd="56dp"
                            android:fontFamily="@font/ubuntu_regular"


                            android:text="4200 SAR"
                            android:textColor="@color/white"
                            android:textSize="20sp"
                            app:layout_constraintEnd_toEndOf="parent"

                            app:layout_constraintTop_toTopOf="parent" />


                </androidx.constraintlayout.widget.ConstraintLayout>


            </androidx.constraintlayout.widget.ConstraintLayout>


        </androidx.core.widget.NestedScrollView>

        <com.google.android.material.button.MaterialButton
                android:id="@+id/confirm_btn"
                style="@style/Widget.AppCompat.Button.Borderless"
                android:layout_width="156dp"


                android:layout_height="60dp"

                android:layout_marginBottom="24dp"

                android:background="@color/colorPrimary"
                android:fontFamily="@font/ubuntu_medium"

                android:gravity="center"

                android:text="@string/finish"
                android:textAllCaps="false"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Button"
                android:textColor="@color/purple"
                android:textSize="16sp"


                android:theme="@style/Theme.MaterialComponents"

                app:backgroundTint="@color/white"
                app:cornerRadius="12dp"
                app:iconGravity="textStart"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:strokeColor="@color/white"
                app:strokeWidth="1dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>


</layout>